{
  "manifest_version": 3,
  "name": "CFV Deck Exporter",
  "description": "Export Cardfight!! Vanguard decks from Decklog to the Cardmarket format",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://decklog-en.bushiroad.com/view/*"],
      "js": ["index.js"],
      "run_at": "document_end"
    }
  ]
}
